{
    "name": "ws/admin",
    "description": "WS admin",
    "keywords": ["yii2", "Winner Softwares", "WS", "WS Admin"],
    "homepage": "https://www.winnersoftwares.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
      
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.0",
        "yiisoft/yii2": "~2.0.52",
        "yiisoft/yii2-bootstrap4": "^2.0",
        "fedemotta/yii2-widget-datatables": "*",
        "bower-asset/datatables.net": "^1.10",
        "blacksmoke26/yii2cdn": "*",
        "dominus77/yii2-sweetalert2-widget": "~1.3",
        "aryelds/yii2-sweet-alert": "@dev",
        "kartik-v/yii2-widget-select2": "@dev",
        "ext-gd": "*",
        "kartik-v/yii2-validators": "@dev",
        "bizley/migration": "^4.3.1",
        "bedezign/yii2-audit": "1.2.8",
        "sandritsch91/yii2-widget-form-wizard": "^1.2"
    },
    "replace": {
    "bower-asset/datatables": "self.version"
},
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.2.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "^5.0.0 || ^4.0",
        "codeception/module-asserts": "^3.0 || ^1.1",
        "codeception/module-yii2": "^1.1",
        "codeception/module-filesystem":  "^3.0 || ^2.0 || ^1.1",
        "codeception/verify": "^3.0 || ^2.2",
        "codeception/lib-innerbrowser": "^4.0 || ^3.0 || ^1.1"
      
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
